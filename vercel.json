{
	"version": 2,
	"builds": [
		{
			"src": "apps/server/package.json",
			"use": "@vercel/node",
			"config": {
				"buildCommand": "bun run build --filter=server",
				"outputDirectory": "apps/server/dist"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/apps/server/dist/src/index.js"
		}
	]
}
