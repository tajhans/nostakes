{
	"name": "server",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"build": "tsc && tsc-alias",
		"check-types": "tsc --noEmit",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/src/index.js",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.787.0",
		"@hono/node-server": "^1.14.1",
		"@hono/trpc-server": "^0.3.4",
		"@trpc/server": "^11.0.0",
		"@types/bun": "^1.2.6",
		"@types/pg": "^8.11.11",
		"@types/sharp": "^0.32.0",
		"@upstash/redis": "^1.34.7",
		"better-auth": "^1.2.5",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.38.4",
		"hono": "^4.7.6",
		"nanoid": "^5.1.5",
		"pg": "^8.14.1",
		"plaiceholder": "^3.0.0",
		"resend": "^4.2.0",
		"sharp": "^0.34.1",
		"ws": "^8.18.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"tsc-alias": "^1.8.11",
		"typescript": "^5.8.2",
		"drizzle-kit": "^0.30.5"
	}
}
